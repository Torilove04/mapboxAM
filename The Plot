import numpy as np
import matplotlib.pyplot as plt
import csv
year = [ENTER YOUR YEARS]
weather = {ENTER YOUR WEATHER DATA}

fig, ax = plt.subplots()
ax.stackplot(year, weather.values(),
             labels=weather.keys(), alpha=0.8)
ax.legend(loc='upper left')
ax.set_title('Eminence IN Weather')
ax.set_xlabel('Year')
ax.set_ylabel('Weather')

plt.show()
